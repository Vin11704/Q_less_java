<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".restaurant1">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="vertical"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/store1_arrow"
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/arrow"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/store1_search"
                    android:layout_width="77dp"
                    android:layout_height="51dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginRight="3dp"
                    android:src="@drawable/search"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/store1_profile"
                    android:layout_width="84dp"
                    android:layout_height="91dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="52dp"
                    android:src="@drawable/store1_burger"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/store1_fav"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/fav"
                    app:layout_constraintStart_toStartOf="@+id/store1_profile"
                    app:layout_constraintTop_toBottomOf="@+id/store1_profile" />

                <ImageView
                    android:id="@+id/store1_duration"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/duration"
                    app:layout_constraintStart_toStartOf="@+id/store1_fav"
                    app:layout_constraintTop_toBottomOf="@+id/store1_fav" />

                <ImageView
                    android:id="@+id/store1_pickup"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    app:layout_constraintStart_toStartOf="@+id/store1_duration"
                    app:layout_constraintTop_toBottomOf="@+id/store1_duration"
                    app:srcCompat="@drawable/pickup" />

                <ImageView
                    android:id="@+id/store1_location"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    app:layout_constraintStart_toStartOf="@+id/store1_pickup"
                    app:layout_constraintTop_toBottomOf="@+id/store1_pickup"
                    app:srcCompat="@drawable/location" />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="36dp"
                    android:text="McYouTwit Burgers and Fries"
                    android:textColor="@color/q_less__color"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/store1_profile"
                    app:layout_constraintTop_toTopOf="@+id/store1_profile" />

                <TextView
                    android:id="@+id/store1_fav_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:paddingTop="2dp"
                    android:text="4.3"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toEndOf="@+id/store1_fav"
                    app:layout_constraintTop_toTopOf="@+id/store1_fav" />

                <TextView
                    android:id="@+id/store1_duration_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:paddingTop="2dp"
                    android:text="Pickup in 11 minutes"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toEndOf="@+id/store1_duration"
                    app:layout_constraintTop_toBottomOf="@+id/store1_fav" />

                <TextView
                    android:id="@+id/store1_pickup_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:paddingTop="2dp"
                    android:text="0.2km away"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toEndOf="@+id/store1_pickup"
                    app:layout_constraintTop_toBottomOf="@+id/store1_duration" />

                <TextView
                    android:id="@+id/store1_location_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:paddingTop="2dp"
                    android:text="8 Somapah Rd, Singapore 487372"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toEndOf="@+id/store1_location"
                    app:layout_constraintTop_toBottomOf="@+id/store1_pickup" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="6dp"
                    android:text="| 102 ratings"
                    android:textSize="10sp"
                    app:layout_constraintStart_toEndOf="@+id/store1_fav_textView"
                    app:layout_constraintTop_toTopOf="@+id/store1_fav_textView" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="124dp"
                android:layout_margin="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="5dp"
                app:cardElevation="15dp">

                <ImageView
                    android:id="@+id/store1_menu1_profile"
                    android:layout_width="107dp"
                    android:layout_height="114dp"
                    android:layout_margin="5dp"
                    android:background="@color/white"
                    android:elevation="3dp"
                    android:src="@drawable/store1_burger" />

                <TextView
                    android:id="@+id/store1_menu1_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="5dp"
                    android:text="Signature McYouTwit Burger"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/store1_menu1_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="30dp"
                    android:text="@string/store1_menu1_description" />

                <TextView
                    android:id="@+id/store1_menu1_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="90dp"
                    android:text="@string/store1_menu1_price" />

                <ImageView
                    android:id="@+id/add_to_chart_11"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="90dp"
                    app:srcCompat="@drawable/add_to_cart" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="124dp"
                android:layout_margin="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="5dp"
                app:cardElevation="15dp">

                <ImageView
                    android:id="@+id/add_to_chart_12"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="90dp"
                    app:srcCompat="@drawable/add_to_cart" />

                <ImageView
                    android:id="@+id/store1_menu2_profile"
                    android:layout_width="107dp"
                    android:layout_height="114dp"
                    android:layout_margin="5dp"
                    android:background="@color/white"
                    android:elevation="3dp"
                    android:src="@drawable/store1_menu2" />

                <TextView
                    android:id="@+id/store1_menu2_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="5dp"
                    android:text="@string/store1_menu2_title"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/store1_menu2_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="30dp"
                    android:text="@string/store1_menu2_description" />

                <TextView
                    android:id="@+id/store1_menu2_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="90dp"
                    android:text="@string/store1_menu2_price" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="124dp"
                android:layout_margin="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="5dp"
                app:cardElevation="15dp">

                <ImageView
                    android:id="@+id/add_to_chart_13"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="90dp"
                    app:srcCompat="@drawable/add_to_cart" />

                <ImageView
                    android:id="@+id/store1_menu3_profile"
                    android:layout_width="107dp"
                    android:layout_height="114dp"
                    android:layout_margin="5dp"
                    android:background="@color/white"
                    android:elevation="3dp"
                    android:src="@drawable/store1_menu3"/>

                <TextView
                    android:id="@+id/store1_menu3_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="5dp"
                    android:text="@string/store1_menu3_title"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/store1_menu3_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="30dp"
                    android:text="@string/store1_menu3_description" />

                <TextView
                    android:id="@+id/store1_menu3_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="90dp"
                    android:text="@string/store1_menu3_price" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="124dp"
                android:layout_margin="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="5dp"
                app:cardElevation="15dp">

                <ImageView
                    android:id="@+id/add_to_chart_14"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="90dp"
                    app:srcCompat="@drawable/add_to_cart" />

                <ImageView
                    android:id="@+id/store1_menu4_profile"
                    android:layout_width="107dp"
                    android:layout_height="114dp"
                    android:layout_margin="5dp"
                    android:background="@color/white"
                    android:elevation="3dp"
                    android:src="@drawable/store1_menu4" />

                <TextView
                    android:id="@+id/store1_menu4_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="5dp"
                    android:text="@string/store1_menu4_title"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/store1_menu4_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="30dp"
                    android:text="@string/store1_menu4_description" />

                <TextView
                    android:id="@+id/store1_menu4_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="90dp"
                    android:text="@string/store1_menu4_price" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="124dp"
                android:layout_margin="10dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="5dp"
                app:cardElevation="15dp">

                <ImageView
                    android:id="@+id/add_to_chart_15"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="90dp"
                    app:srcCompat="@drawable/add_to_cart" />

                <ImageView
                    android:layout_width="107dp"
                    android:layout_height="114dp"
                    android:layout_margin="5dp"
                    android:background="@color/white"
                    android:elevation="3dp"
                    android:src="@drawable/store1_burger" />

                <TextView
                    android:id="@+id/store1_menu5_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="5dp"
                    android:text="Signature McYouTwit Burger"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/store1_menu5_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="30dp"
                    android:text="@string/store1_menu1_description" />

                <TextView
                    android:id="@+id/store1_menu5_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="130dp"
                    android:paddingTop="90dp"
                    android:text="@string/store1_menu1_price" />

            </androidx.cardview.widget.CardView>


        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>